{
  "name": "react-dynamic-scroll-menu",
  "version": "1.0.1",
  "description": "A simple menu for sections of a single site.",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "start": "rollup -c -w",
    "dev": "vite",
    "prepublish": "npm run build",
    "cy:run": "cypress run -- --browser chrome --e2e 'cypress/e2e/'",
    "cy:open": "cypress open"
  },
  "keywords": [
    "menu",
    "scroll",
    "react",
    "typescript"
  ],
  "author": "JonasM",
  "type": "module",
  "license": "ISC",
  "homepage": "https://github.com/JonasMa/react-scroll-menu",
  "files": [
    "dist",
    "src"
  ],
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18",
    "react-dom": "^16.8.0 || ^17 || ^18"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cypress": "^13.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.17.2",
    "rollup-plugin-sass": "^1.12.22",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
